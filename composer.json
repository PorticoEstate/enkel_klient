{
	"name": "bergenkommune/enkelklient",
	"description": "Client application for API integration",
	"type": "project",
	"require": {
		"php": "^8.3 || ^8.4",
		"slim/slim": "^4.14",
		"slim/psr7": "^1.7",
		"php-di/php-di": "^7.0",
		"smarty/smarty": "^4.3",
		"components/font-awesome": "^5.15",
		"oomphinc/composer-installers-extender": "^2.0",
		"components/jquery": "^3.7",
		"ezyang/htmlpurifier": "^4.14",
		"vlucas/phpdotenv": "^5.4",
		"twbs/bootstrap": "^5.3",
		"jumbojett/openid-connect-php": "^1.0",
		"php-di/slim-bridge": "^3.4"
	},
	"config": {
		"allow-plugins": {
			"composer/installers": true,
			"oomphinc/composer-installers-extender": true
		}
	},
	"autoload": {
		"psr-4": {
			"App\\": "src/"
		}
	}
}
